# Provisioning for Grandstream *requires* mod_rewrite in order to work!

<IfModule mod_rewrite.c>
	RewriteEngine on
	
	# The phone asks for
	# cfg<MAC-addr>
	
	# Rewrite requests for cfg<MAC-addr>
	RewriteRule ^cfg(000b82[0-9a-f]*)$ settings.php?mac=$1 [L]
	
</IfModule>
