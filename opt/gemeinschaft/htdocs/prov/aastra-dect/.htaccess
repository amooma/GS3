# Provisioning for Aastra DECT *requires* mod_rewrite in order to work!

<IfModule mod_rewrite.c>
	RewriteEngine on
	
	# The OMM asks for
	# user_common.cfg
	# <extension>.cfg
	
	RewriteRule ^user_common\.cfg$ settings.php [L]
	RewriteRule ^([0-9]*)\.cfg$ settings.php?ext=$1 [L]
	
</IfModule>
